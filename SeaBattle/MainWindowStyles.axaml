<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style Selector="Window">
        <Setter Property="Background" Value="{DynamicResource MainBackground}"/>
    </Style>
    <Style Selector="Button.regular">
        <Setter Property="Background" Value="{DynamicResource RegularColor}"/>
    </Style>
    <Style Selector="Button.error">
        <Setter Property="Background" Value="{DynamicResource ErrorColor}"/>
    </Style>
    <Style Selector="Button.miss">
        <Setter Property="Background" Value="{DynamicResource MissColor}"/>
    </Style>
    <Style Selector="Button.hit">
        <Setter Property="Background" Value="{DynamicResource MissColor}"/>
    </Style>
    <Style Selector="Button.fullhit">
        <Setter Property="Background" Value="Transparent"/>
    </Style>
    <Style Selector="Button.disabled">
        <Setter Property="Background" Value="{DynamicResource DisabledColor}"/>
    </Style>
</Styles>
